# MySQL Database Settings
MYSQL_SERVER=localhost
MYSQL_USERNAME=your_db_username
MYSQL_PASSWORD=your_db_password
MYSQL_DB=resourcespace
READ_ONLY_DB_USERNAME=resourcespace_r
READ_ONLY_DB_PASSWORD=your_r_password
MYSQL_BIN_PATH=/usr/bin

# Base URL
BASE_URL=http://your-domain.com/resourcespace

# Email Configuration
USE_PHPMAILER=true
USE_SMTP=true

# SMTP Settings
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_AUTH=true
SMTP_USERNAME=your-email@domain.com
SMTP_PASSWORD=your-app-password
SMTP_SECURE=tls
EMAIL_FROM=your-email@domain.com
EMAIL_FROM_NAME=ResourceSpace
EMAIL_NOTIFY=your-email@domain.com
